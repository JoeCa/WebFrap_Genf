      <text lang="de" >Deutsches Label</text>
      <text lang="de" >English Label</text>
    </label>
    
    <!-- 
      Liste der Attribute
     -->
    <attributes>
      <attribute name="an_attribute" type="text" size="200" ></attribute>
    </attributes>
    
    
    <references>
    
    </references>
    
    
    </entity>
  </entities>

