<?xml version="1.0" encoding="UTF-8"?>
<bdl>

  <!-- befinde sich immer in entity  -->
  <entities>
  
    <!-- 
      das attribute name ist required 
      wenn 2 entitÃ¤ten den gleichen namen haben werden sie beim
      interpretieren gemerged, dabei gilt last write win
        
    -->
    <entity name="name" >
    
    <!-- 
      optional,
      Mindestens das Englische Label muss vorhanden sein,
      wenn ein Label definiert wird 
    -->
